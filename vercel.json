
{
  "crons": [
    {
      "path": "/api/cron/cleanup-images",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/reset-smtp-quotas",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/check-stalled-campaigns",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/process-scheduled-campaigns",
      "schedule": "*/5 * * * *"
    }
  ],
  "functions": {
    "app/api/webhooks/process-campaign/route.ts": {
      "maxDuration": 60
    },
    "app/api/email/process-next/[id]/route.ts": {
      "maxDuration": 60
    },
    "app/api/email/restart/[id]/route.ts": {
      "maxDuration": 30
    },
    "app/api/email/reset/[id]/route.ts": {
      "maxDuration": 30
    },
    "app/api/email/process-now/[id]/route.ts": {
      "maxDuration": 30
    },
    "app/api/email/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/webhooks/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache"
        }
      ]
    },
    {
      "source": "/api/track/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}